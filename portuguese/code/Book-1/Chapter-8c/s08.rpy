translate portuguese b1c08cs08_ll_punishment_85f02434:

    # ih "You. Come here and assist us."
    ih "Você. Venha aqui nos ajudar."

# game/code/Book-1/Chapter-8c/s08.rpy:511
translate portuguese b1c08cs08_ll_punishment_fc3543a8:

